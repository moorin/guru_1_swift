import Foundation

func timesTable(x: Int)
{
    for i in 1...9
    {
        print("\(x)*\(i)=\(x*i)")
    }
}

print("단 입력")

if let number:String = readLine()
{
    if let realNum: Int = Int(number)
    {
        timesTable(x: realNum)
    }
    
}
else{
    print("입력오류")
}
